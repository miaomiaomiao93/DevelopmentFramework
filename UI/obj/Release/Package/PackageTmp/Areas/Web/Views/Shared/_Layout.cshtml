<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/Plugs/Ali-Font/iconfont.css" rel="stylesheet" />
    <link href="~/Content/Web/style/web_Layout.css" rel="stylesheet" />
    <script src="https://cdn.bootcss.com/vue/2.3.4/vue.js"></script>
    <script src="https://cdn.bootcss.com/jquery/2.0.0/jquery.min.js"></script>
    <script src="~/Content/Web/script/web_PageBar.js"></script>
    <link href="~/Content/Web/style/web_PageBar.css" rel="stylesheet" />
    <link href="~/Content/Web/style/web_TreeView.css" rel="stylesheet" />
    <script src="~/Content/Web/script/web_M_Layout.js"></script>
</head>
<body>
    <div class="pack">
        <div class="container">
            <div class="search">
                <input type="text" v-model="modelName" /><!--否则有换行符
                --><button v-on:click="search"></button>
            </div>
            <div class="content">
                <div class="content-info">
                    @RenderBody()
                </div><!--否则有换行符
                --><div class="nav">
                    <div id="menuTree" class="menuTree"></div>
                </div>
            </div>
        </div>
    </div>
    <script src="~/Content/Web/script/web_TreeView.js"></script>
    <script src="~/Content/Web/script/web_VM_Layout.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>
